{
	"categories": [
		"Downloaders",
		"Books"
	],
	"description": "Readarr is an eBook and audiobook collection manager for Usenet and BitTorrent users.",
	"env": [
		{
			"default": "1000",
			"label": "PUID",
			"name": "PUID"
		},
		{
			"default": "1000",
			"label": "PGID",
			"name": "PGID"
		},
		{
			"default": "America/Chicago",
			"label": "Time Zone",
			"name": "TZ"
		}
	],
	"image_arm64": "lscr.io/linuxserver/readarr:develop",
	"image_amd64": "lscr.io/linuxserver/readarr:develop",
	"logo": "https://github.com/Jarrodb22/BearLAN/blob/main/images/readarr.png",
	"name": "readarr",
	"officialDoc": "https://wiki.servarr.com/en/readarr",
	"platform": "linux",
	"ports": [
		"8787:8787/tcp"
	],
	"restart_policy": "unless-stopped",
	"title": "readarr",
	"type": 1,
	"volumes": [
		{
			"bind": "/portainer/readarr/config",
			"container": "/config"
		},
		{
			"bind": "/mnt/media/Books",
			"container": "/books"
		},
		{
			"bind": "mnt/media/downloads",
			"container": "/downloads"
		}
	],
	"webpage": "https://readarr.com/"
}